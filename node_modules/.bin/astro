#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/spore/website/sporestudio.github.io/node_modules/.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules/astro/node_modules:/home/spore/website/sporestudio.github.io/node_modules/.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules:/home/spore/website/sporestudio.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/spore/website/sporestudio.github.io/node_modules/.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules/astro/node_modules:/home/spore/website/sporestudio.github.io/node_modules/.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules:/home/spore/website/sporestudio.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@4.16.0_@types+node@22.7.5_rollup@4.24.0_typescript@5.6.3/node_modules/astro/astro.js" "$@"
fi
